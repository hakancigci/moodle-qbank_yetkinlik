<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/bank/yetkinlik/db" VERSION="2026030102" COMMENT="Tablo qbank eklentisi tarafından yönetiliyor">
    <TABLES>
        <TABLE NAME="local_yetkinlik_qmap" COMMENT="Soru ve yetkinlik eşleştirme tablosu">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="courseid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" COMMENT="Kurs ID"/>
                <FIELD NAME="questionid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" COMMENT="Soru ID"/>
                <FIELD NAME="competencyid" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" COMMENT="Yetkinlik ID"/>
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" COMMENT="Oluşturulma Tarihi"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
                <KEY NAME="coursefk" TYPE="foreign" FIELDS="courseid" REFTABLE="course" REFFIELDS="id"/>
                <KEY NAME="questionfk" TYPE="foreign" FIELDS="questionid" REFTABLE="question" REFFIELDS="id"/>
                </KEYS>
            <INDEXES>
                <INDEX NAME="course_question_idx" UNIQUE="false" FIELDS="courseid, questionid"/>
            </INDEXES>
        </TABLE>
    </TABLES>
</XMLDB>